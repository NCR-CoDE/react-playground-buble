{
  "version": "0.4.0",
  "scripts": {
    "publish:internal": "node publish-updated react-playground-bare && node publish-updated react-playground-lite --new && node publish-updated react-playground-styled --new && node publish-updated di-ui-icon-elements --new && node publish-updated di-ui-styled --new",
    "publish:pre-npm": "lerna publish --skip-npm",
    "clean": "rimraf packages/$PACKAGE/lib packages/$PACKAGE/dist packages/$PACKAGE/lib-es",
    "build:webpack-var": "npm run clean && NODE_ENV=production webpack --config packages/$PACKAGE/webpack.config.js --env.var",
    "build:webpack": "npm run clean && cd packages/$PACKAGE && NODE_ENV=production ../../node_modules/webpack --config ../../webpack.config.js --env.prod --context packages/$PACKAGE/src && cp packages/$PACKAGE/examples/index.html packages/$PACKAGE/dist",
    "build:babel": "npm run clean && NODE_ENV=lib babel -D packages/$PACKAGE/src -d packages/$PACKAGE/lib",
    "build:es": "NODE_ENV=es babel -D packages/$PACKAGE/src -d packages/$PACKAGE/lib-es",
    "build": "npm run clean && npm run build:webpack",
    "start": "node packages/$PACKAGE/devServer.js",
    "lint": "eslint src",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "test": "echo test",
    "deploy": "echo NOOP"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-istanbul": "^1.0.3",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-regenerator": "^6.16.1",
    "babel-plugin-webpack-loaders": "^0.8.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-es2015-rollup": "^1.2.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-1": "^6.5.0",
    "babel-register": "^6.9.0",
    "body-parser": "^1.15.2",
    "buble": "^0.14.2",
    "cm-show-invisibles": "^1.2.1",
    "component-playground": "^1.3.1",
    "compression": "^1.6.1",
    "cross-env": "^1.0.7",
    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-flowtype": "^2.3.0",
    "eslint-plugin-import": "^1.10.0",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-react": "^5.2.2",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.13.3",
    "file-loader": "^0.8.5",
    "isomorphic-fetch": "^2.2.1",
    "json-loader": "^0.5.4",
    "lerna": "2.0.0-beta.32",
    "node-noop": "^1.0.0",
    "raw-loader": "^0.5.1",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.2.1",
    "react-dom": "^15.4.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "recompose": "^0.20.2",
    "rimraf": "^2.4.3",
    "styled-components": "^1.4.0",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.0",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.17.1"
  }
}
