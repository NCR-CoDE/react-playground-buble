{
  "version": "0.0.9",
  "name": "@di-internal/di-ui-styled",
  "description": "React playground using Buble",
  "main": "lib/index.js",
  "browser:ie9": "lib/index-ie9.js",
  "author": "Phil Holden (http://github.com/philholden)",
  "license": "NCR",
  "publishConfig": {
    "registry": "http://anprd10d2dge140.dca.diginsite.net:8080/repository/npm-internal"
  },
  "scripts": {
    "clean": "rimraf lib dist lib-es",
    "svgo": "node ./node_modules/svgo/bin/svgo --pretty -f input/svg/current-color/ -o output/current-color",
    "start": "cd ../.. && PACKAGE=$npm_package_config_package node ./dev-server.js",
    "test": "cd ../.. && PACKAGE=$npm_package_config_package npm test",
    "build:babel": "cd ../.. && PACKAGE=$npm_package_config_package npm run build:babel",
    "build:webpack": "npm run clean && NODE_ENV=production webpack --config webpack.config.local.js --env.prod && cp examples/index.html dist",
    "npmdocs": "cd ../.. && PACKAGE=$npm_package_config_package npm run npmdocs",
    "prepublish": "npm run build:babel"
  },
  "dependencies": {
    "@di-internal/di-ui-icon-elements": "^0.0.2",
    "color-array": "0.0.8",
    "react-addons-transition-group": "^15.4.1",
    "react-docgen": "^2.13.0",
    "react-motion": "^0.4.7",
    "react-pure-render": "^1.0.2",
    "recompose": "^0.20.2",
    "styled-components": "^1.4.0"
  },
  "devDependencies": {
    "@di-internal/react-playground-lite": "^0.0.4",
    "@di-internal/react-playground-styled": "^0.0.4",
    "fastclick": "^1.0.6",
    "react-router-dom": "^4.0.0-beta.5",
    "rimraf": "^2.6.1",
    "webpack": "^2.2.0"
  },
  "peerDependencies": {
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  },
  "config": {
    "package": "di-ui-styled",
    "port": 3000
  },
  "files": [
    "package.json",
    "lib",
    "readme.md",
    "lib-es"
  ]
}
